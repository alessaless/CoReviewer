{
    "version": "1.0.1",
    "manifest_version": 3,
    "name": "Code Review Extension",
    "description": "This is a Chrome extension built with React and TypeScript",
    "action": {
        "default_popup": "js/index.html",
        "default_title": "Code Review Extension",
        "default_icon": {
            "16": "favicon16.png",
            "32": "favicon32.png"
        }
    },
    "host_permissions": [
        "http://localhost:5001/*"
    ],
    "web_accessible_resources": [{
        "matches": ["<all_urls>"],
        "resources": ["logoCoReviewerBianco.png"]
    }]
}